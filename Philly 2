UICornerDiscord.Parent = DiscordButton
UICornerDiscord.CornerRadius = UDim.new(0, 8)

-- Discord Button Hover Effect
DiscordButton.MouseEnter:Connect(function()
    TweenService:Create(DiscordButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(134, 157, 238)}):Play()
end)
DiscordButton.MouseLeave:Connect(function()
    TweenService:Create(DiscordButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(114, 137, 218)}):Play()
end)

-- Get Key Button (with Hover Effect)
GetKeyButton.Parent = Frame
GetKeyButton.Size = UDim2.new(0, 280, 0, 40)
GetKeyButton.Position = UDim2.new(0.5, -140, 0, 210)
GetKeyButton.Text = "Get Key"
GetKeyButton.BackgroundColor3 = Color3.fromRGB(70, 130, 255)
GetKeyButton.TextColor3 = Color3.fromRGB(255, 255, 255)
