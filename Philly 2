GetKeyButton.MouseEnter:Connect(function()
    TweenService:Create(GetKeyButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(90, 150, 255)}):Play()
end)
GetKeyButton.MouseLeave:Connect(function()
    TweenService:Create(GetKeyButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(70, 130, 255)}):Play()
end)

-- Notifications function
local function notify(message)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Exodus Scripts";
        Text = message;
        Duration = 3;
    })
end

-- Discord Button click
DiscordButton.MouseButton1Click:Connect(function()
    setclipboard("discord.gg/exodushub")
    notify("Discord link copied!")
end)

-- Get Key Button click
GetKeyButton.MouseButton1Click:Connect(function()
    setclipboard("https://ads.luarmor.net/get_key?for=Philly2_glitched_character-rtoftdfpIAxK")
    notify("Key link copied!")
end)

-- Enter Button click
EnterButton.MouseButton1Click:Connect(function()
    local entered_key = TextBox.Text
    if entered_key ~= "" then
        local success, err = pcall(function()
            local loader_code = game:HttpGet("https://api.luarmor.net/files/v4/loaders/8c69e4b9ba4e1b6b7e7f525b1c69137a.lua")
            local full_script = "script_key = '" .. entered_key .. "';" .. loader_code
            loadstring(full_script)()
        end)
        if not success then
            notify("Error executing script: " .. tostring(err))
        end
        ScreenGui:Destroy()
    else
        EnterButton.Text = "Please enter a key!"
        task.wait(1.5)
        EnterButton.Text = "Enter"
    end
end)
